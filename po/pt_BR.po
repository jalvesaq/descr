#, c-format
msgid "Error: could not read file \"%s\".\n"
msgstr "Erro: não foi possível ler o arquivo \"%s\".\n"

#: descr/src/descr.c:69
#, c-format
msgid "Error: could not write file \"%s\".\n"
msgstr "Erro: não foi possível gravar o arquivo \"%s\".\n"

#: descr/src/descr.c:98
#, c-format
msgid "Error: line %d has only %d characters.\n"
msgstr "Erro: linha %d tinha somente %d caracteres.\n"

#: descr/src/descr.c:142
#, c-format
msgid "%d lines written in \"%s\".\n"
msgstr "%d linhas gravadas em \"%s\".\n"

#: descr/src/descr.c:144
#, c-format
msgid "One line from \"%s\" skipped because shorter than 3 characters.\n"
msgstr "Uma linha de \"%s\" não lida porque menor do que 3 caracteres.\n"

#: descr/src/descr.c:147
#, c-format
msgid "%d lines from \"%s\" skipped because shorter than 3 characters.\n"
msgstr "%d linhas de \"%s\" não lidas porque menores do que 3 caracteres.\n"
